package com.test.java;

//1. 9개 자료형을 기반으로
//2. 주변의 데이터를 선택
//3. 자료형을 선택하고 변수를 생성하고 데이터를 대입하고 화면에 출력한다.
//4. 각 자료형 x 10개
public class Ex05_DataType {
	public static void main(String[] args)
	{
		byte numberOfFinger = 10;
		byte numberOfMonitor = 2;
		short zoomPID = 15704;
		int sizeOfJavaexe = 277632; //277,632Byte
		long lottoWin = 22252809381L; //20210327
		float currentTemperature = 18.3F; //Celsius
		double waterCapacity = 500.0D; //liter
		boolean isMan = true;
		char lastOfASCII = 127;
		String myName = "김형래";
		
		System.out.println(numberOfFinger);
		System.out.println(numberOfMonitor);
		System.out.println(zoomPID);
		System.out.println(sizeOfJavaexe);
		System.out.println(lottoWin);
		System.out.println(currentTemperature);
		System.out.println(waterCapacity);
		System.out.println(isMan);
		System.out.println(lastOfASCII);
		System.out.println(myName);
		
	}
}
